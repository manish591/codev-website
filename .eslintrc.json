{
    "plugins": ["react", "react-hooks", "prettier", "@typescript-eslint"],
    "extends": ["eslint:recommended", "next", "next/core-web-vitals", "prettier"],
    "env": {
        "jest": true
    },
    "rules": {
        "indent": ["error", 4],
        "quotes": ["error", "double"],
        "semi": ["error", "always"],
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": "error"
    }
}
